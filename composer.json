{
    "name": "ck-developer/php-types",
    "description": "php types",
    "type": "library",
    "keywords": ["php", "types", "email", "url", "currency"],
    "homepage": "https://github.com/ck-developer/php-types",
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Khedhiri",
            "email": "claude@khedhiri.com",
            "homepage": "https://www.khedhiri.com"
        }
    ],
    "support": {
        "docs": "https://ck-developer.github.io/php-types/",
        "issues": "https://github.com/ck-developer/php-types/issues",
        "rss": "https://github.com/ck-developer/php-types/releases.atom",
        "source": "https://github.com/ck-developer/php-types.git",
        "wiki": "https://github.com/ck-developer/php-types/wiki"
    },
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit --verbose --colors=always --coverage-html build/html",
        "test-ci": "phpunit --verbose --coverage-clover build/logs/clover.xml"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
